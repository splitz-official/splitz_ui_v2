{
  "expo": {
    "name": "NewScreens",
    "slug": "NewScreens",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splitzlogo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#005D1A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/dm-sans/DMSans_400Regular.ttf",
            "node_modules/@expo-google-fonts/dm-sans/DMSans_500Medium.ttf",
            "node_modules/@expo-google-fonts/dm-sans/DMSans_700Bold.ttf"
          ]
        }
      ],
      "expo-secure-store"
    ]
  }
}
